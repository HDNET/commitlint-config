{
    "name": "@hdnet/commitlint-config",
    "version": "0.1.3",
    "description": "HDNET commitlint-config, implementing HSR-3",
    "author": "HDNET",
    "keywords": [
        "commitlint",
        "hdnet",
        "HSR"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/HDNET/commitlint-config.git"
    },
    "main": "index.js",
