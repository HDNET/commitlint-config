        "@commitlint/config-conventional": "^17.4.4"
    },
    "peerDependencies": {
        "@commitlint/lint": ">= 17"
    },
    "devDependencies": {
        "@commitlint/cli": "^17.5.1",
        "husky": "^8.0.3"
    }
}
